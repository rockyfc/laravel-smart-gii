{
    "name": "rockyfc/laravel-smart-gii",
    "description": "laravel gii项目，生成符合规范的CURD代码",
    "keywords": ["laravel", "extension", "gii"],
    "license": "MIT",
    "authors": [
        {
            "name": "rockyfc",
            "email": "softfc@163.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "laravel/framework": "^6.0|^7.0|^8.0",
        "rockyfc/laravel-smart-common": "dev-master",
        "barryvdh/reflection-docblock": "^2.0.6"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Smart\\Gii\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {

        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Smart\\Gii\\Providers\\GiiServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev-master",
    "prefer-stable": true
}
